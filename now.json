{
    "version": 2,
    "public": false,
    "name": "guac-web",
    "alias": "guac.live",
    "regions": ["arn", "cdg", "lhr", "bru"],
    "routes": [
      { "src": "/c/(?<name>[^/]+)$", "dest": "/c?name=$name" },
      { "handle": "filesystem" },
      { "src": "/.*", "status": 404, "dest": "/static/404.html" }  
    ],
    "build": {
      "env": {
        "API_URL": "@guac-api-url",
        "CHAT_URL": "@guac-chat-url",
        "GIPHY_API_KEY": "@guac-giphy-api-key"
      }
    },
    "builds": [{ "src": "package.json", "use": "@now/next@canary", "config": {"maxLambdaSize": "10mb"} }]
}  